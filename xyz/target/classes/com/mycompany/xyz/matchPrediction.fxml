<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="575.0" prefWidth="882.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.xyz.MatchPredictionController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Pane prefHeight="540.0" prefWidth="842.0">
         <children>
            <ComboBox fx:id="team1" layoutX="69.0" layoutY="54.0" prefWidth="150.0" />
            <ComboBox fx:id="team2" layoutX="630.0" layoutY="54.0" prefWidth="150.0" />
            <TextField fx:id="team1_name" layoutX="69.0" layoutY="129.0" />
            <TextField fx:id="team2_name" layoutX="631.0" layoutY="129.0" />
            <TextField fx:id="win_percentage_team1" layoutX="69.0" layoutY="329.0" />
            <TextField fx:id="win_percentage_team2" layoutX="631.0" layoutY="329.0" />
            <Button fx:id="get_prediction" layoutX="506.0" layoutY="436.0" mnemonicParsing="false" onAction="#get_Prediction" text="GET PREDICTION" />
            <Button layoutX="319.0" layoutY="436.0" mnemonicParsing="false" onAction="#home_btn" text="HOME" />
         </children>
      </Pane>
   </children>
</VBox>
