<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="523.0" prefWidth="489.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.xyz.PrimaryController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Pane prefHeight="359.0" prefWidth="449.0">
         <children>
            <Button fx:id="add_player" layoutX="165.0" layoutY="113.0" mnemonicParsing="false" onAction="#teamScreen" prefHeight="25.0" prefWidth="131.0" text="Add Player" />
            <Button fx:id="add_match" layoutX="165.0" layoutY="167.0" mnemonicParsing="false" onAction="#addMatchScreen" prefHeight="25.0" prefWidth="131.0" text="Add Match" />
            <Button fx:id="match_prediction" layoutX="165.0" layoutY="218.0" mnemonicParsing="false" onAction="#match_prediction" prefHeight="25.0" prefWidth="131.0" text="Match Prediction" />
            <Button fx:id="team_rank" layoutX="165.0" layoutY="275.0" mnemonicParsing="false" onAction="#team_rankings" prefHeight="25.0" prefWidth="131.0" text="Team Rankings" />
            <Label layoutX="110.0" layoutY="27.0" prefHeight="54.0" prefWidth="254.0" text="CRICKET APPLICATION" textFill="#490be8">
               <font>
                  <Font name="System Bold Italic" size="23.0" />
               </font>
            </Label>
         </children>
      </Pane>
   </children>
</VBox>
